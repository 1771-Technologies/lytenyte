---
import { createGenerator } from "./base";
import { type BaseTypeTableProps } from "./type-table";
import TypeTable from "./type-table.astro";

interface Props extends BaseTypeTableProps {}

const generator = createGenerator();
const props = Astro.props;

const typeProps = await generator.generateTypeTable(props);
---

<TypeTable type={typeProps.flatMap((x) => x.entries)} />
