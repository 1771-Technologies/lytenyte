{
  "name": "@1771technologies/lytenyte-core",
  "description": "Blazingly fast headless React data grid with 100s of features.",
  "license": "Apache-2.0",
  "keywords": [
    "react",
    "data-grid",
    "react-table",
    "headless",
    "virtualized"
  ],
  "version": "1.0.0-beta.0",
  "type": "module",
  "files": [
    "dist",
    "main.css",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://1771technologies.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/1771-technologies/lytenyte.git",
    "directory": "packages/lytenyte-core"
  },
  "bugs": {
    "url": "https://github.com/1771-technologies/lytenyte/issues"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types/types.ts",
      "import": "./src/types/types.ts",
      "require": "./src/types/types.ts"
    },
    "./grid.css": "./main.css"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.js"
      },
      "./types": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/types/index.js",
        "require": "./dist/types/index.js"
      },
      "./grid.css": "./main.css"
    }
  },
  "dependencies": {
    "@1771technologies/atom": "^1.0.2",
    "@1771technologies/lytenyte-dragon": "workspace:*",
    "@1771technologies/lytenyte-shared": "workspace:*",
    "@1771technologies/lytenyte-dom-utils": "workspace:*",
    "@1771technologies/lytenyte-js-utils": "workspace:*",
    "@1771technologies/lytenyte-react-hooks": "workspace:^"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  }
}
